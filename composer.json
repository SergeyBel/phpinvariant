{
	"name": "sergey-bel/phpinvariant",
	"description": "PHP property-based testing framework",
	"license": "MIT",
	"authors": [
		{
			"name": "Sergey Belov"
		}
	],

	"autoload": {
        "psr-4": {
			"PhpInvariant\\": "./src"

		}
    },
	"autoload-dev": {
		"psr-4": {
			"PhpInvariant\\Invariants\\": "./invariants",
			"Custom\\": "./custom",
			"PhpInvariant\\Tests\\": "./tests"
		}
	},
	
    "require": {
		"symfony/dependency-injection": "^6.0",
		"symfony/console": "^6.0",
		"symfony/finder": "^6.0",
		"phpunit/phpunit": "^9.5",
		"nikic/php-parser": "^4.15",
		"symfony/config": "^6.2",
		"symfony/yaml": "^6.2",
		"webmozart/assert": "^1.11"
	},
	"require-dev": {
		"symfony/var-dumper": "^6.0",
		"friendsofphp/php-cs-fixer": "^3.6",
		"phpstan/phpstan": "^1.5"
	},
	"bin": ["bin/phpinvariant"]
}
